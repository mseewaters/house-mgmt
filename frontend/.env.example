# Frontend Environment Variables Template
# Copy this file to .env.development or .env.production and fill in your values

# Application Configuration
VITE_APP_NAME=MyPersonalApp
VITE_ENVIRONMENT=development

# API Configuration  
VITE_API_BASE_URL=http://localhost:3000
# Production example: https://your-api-id.execute-api.us-east-1.amazonaws.com/Prod/

# External API Keys (replace with your actual keys)
VITE_WEATHER_API_KEY=your_weather_api_key_here
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key_here

# Optional: Authentication
VITE_AUTH0_DOMAIN=your-auth0-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your_auth0_client_id

# Optional: Analytics
VITE_GA_TRACKING_ID=GA-XXXXXXXXX